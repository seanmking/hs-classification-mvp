    "db:migrate:sars": "tsx src/db/migrate-to-sars.ts",
    "db:validate": "tsx src/db/seed/validate-staging-data.ts",
    "db:seed:sars": "tsx src/db/seed/seed-sars-data.ts",
    "db:backup": "cp database.db \"database.backup.$(date +%Y%m%d_%H%M%S).db\"",
    "import:sars": "tsx src/scripts/import-sars-data.ts"
  },
